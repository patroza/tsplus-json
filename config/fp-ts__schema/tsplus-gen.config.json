{
  "project": {
    "packageName": "@fp-ts/schema",
    "rootDir": "src",
    "tsconfig": "tsconfig.build.json",
    "exclude": ["**/internal/**"],
    "moduleConfig": {
      "@fp-ts/schema": {
        "staticPrefixes": ["make", "from", "unsafeFrom"],
        "fluentNamespaces": ["fp-ts/schema"]
      }
    }
  },
  "namespaces": [
    {
      "name": "fp-ts/schema",
      "type": {
        "include": true,
        "includeCompanion": true,
        "companionSuffix": ".Ops"
      },
      "static": {
        "include": true,
        "staticSuffix": ".Ops"
      },
      "fluent": {
        "include": true,
        "includeStatic": true,
        "staticSuffix": ".Aspects"
      },
      "pipeable": {
        "include": true,
        "includeStatic": true,
        "staticSuffix": ".Aspects"
      },
      "getter": {
        "include": true,
        "includeStatic": true,
        "staticSuffix": ".Aspects"
      }
    }
  ],
  "additionalExtensions": [
    ["fp-ts/schema/Parser#decode", "getter", "decode", "fp-ts/schema/Schema"],
    ["fp-ts/schema/Parser#decodeOrThrow", "getter", "decodeOrThrow", "fp-ts/schema/Schema"],
    ["fp-ts/schema/Parser#encode", "getter", "encode", "fp-ts/schema/Schema"],
    ["fp-ts/schema/Parser#encodeOrThrow", "getter", "encodeOrThrow", "fp-ts/schema/Schema"],
    ["fp-ts/schema/Parser#is", "getter", "guard", "fp-ts/schema/Schema"]
  ]
}
